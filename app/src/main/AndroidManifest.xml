    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="21" />


    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />


    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name=".Presentacion"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Principal"
            android:label="@string/title_activity_principal" >
        </activity>
        <activity
            android:name=".AltaLugar"
            android:label="@string/title_activity_alta_lugar" >
        </activity>
        <activity
            android:name=".AltaRecordos"
            android:label="@string/title_activity_alta_recordos" >
        </activity>
        <activity
            android:name=".XestionMapa"
            android:label="@string/title_activity_xestion_mapa" >
        </activity>

        <uses-library android:name="com.google.android.maps" />


        <activity
            android:name=".ListaLugares"
            android:label="@string/title_activity_lista_lugares" >
        </activity>
        <activity
            android:name=".PantallaPreferencias"
            android:label="@string/title_activity_pantalla_preferencias" >
        </activity>
    </application>

</manifest>
